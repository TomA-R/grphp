{
  "name": "bigcommerce/grphp",
  "description": "gRPC PHP Framework",
  "type": "library",
  "license": "MIT",
  "repositories": [
    { "type": "vcs", "url": "https://github.com/grpc/grpc-php" }
  ],
  "require": {
    "grpc/grpc": "1.9.*",
    "google/protobuf": "3.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.1.*",
    "phpunit/php-code-coverage": "^5.2@dev",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": { "": "src/"}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.0.30"
    }
  }
}
